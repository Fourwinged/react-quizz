{
  "name": "react-quizz",
  "version": "0.1.0",
  "description": "Small over-engineered React quizz made for a hackathon.",
  "scripts": {
    "start": "./node_modules/.bin/webpack --config config/webpack.production.js && NODE_ENV=production node --harmony server/server.js",
    "build": "./node_modules/.bin/webpack --config config/webpack.production.js",
    "assets": "./node_modules/.bin/webpack --config config/webpack.assets.js",
    "server": "node --harmony server/server.js",
    "lint": "eslint src",
    "dev": "node --harmony server/hot-server.js",
    "test": "mocha --opts test/mocha.opts --compilers js:babel/register --reporter list src/scripts/**/*.test.js"
  },
